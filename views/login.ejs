<body>
    <div id='wrapper'>
        <a href="#" class="button loginItem facebook"><i class="fab fa-facebook-square"></i> Log in with Facebook</a>
        <a href="#" class="button loginItem twitter"><i class="fab fa-twitter-square"></i> Log in with Twitter</a>
        <a href="#" class="button loginItem google"><i class="fab fa-google-plus-square"></i> Log in with Google</a>
        <div class="loginForm">
            <form action="" method="post">
                <input type="text" name="" id="" placeholder="Username" class="loginItem loginInput">
                <input type="password" name="" id="" placeholder="Password" class="loginItem loginInput">
                <button type="submit" class="loginItem login">Login <i class="fas fa-sign-in-alt"></i></button>
            </form>
        </div>
        <a href="#" class="button loginItem signUp">Register <i class="fas fa-address-card"></i></a>

    </div>
